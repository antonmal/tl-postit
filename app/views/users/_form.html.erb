<%= render 'shared/errors', object: @user %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br />
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br />
  <%= f.submit @user.new_record? ? 'Register' : 'Update', class: 'btn btn-success' %>
<% end %>
